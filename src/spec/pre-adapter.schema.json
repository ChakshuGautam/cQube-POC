{
    "$id": "http://example.com/pre-adapter.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [
        "studentId",
        "schoolId",
        "isPresent",
        "grade",
        "section"
    ],
    "properties": {
        "studentId": {
            "title": "The studentId Schema",
            "type": "string",
            "default": "",
            "examples": [
                "test-student-id"
            ]
        },
        "schoolId": {
            "title": "The schoolId Schema",
            "type": "string",
            "default": "",
            "examples": [
                "123"
            ]
        },
        "isPresent": {
            "title": "The isPresent Schema",
            "type": "boolean",
            "default": false,
            "examples": [
                true
            ]
        },
        "grade": {
            "title": "The grade Schema",
            "type": "integer",
            "default": 0,
            "examples": [
                1
            ]
        },
        "section": {
            "title": "The section Schema",
            "type": "string",
            "default": "",
            "examples": [
                "A"
            ]
        }
    },
    "examples": [
        {
            "studentId": "test-student-id",
            "schoolId": "123",
            "isPresent": true,
            "grade": 1,
            "section": "A"
        }
    ]
}