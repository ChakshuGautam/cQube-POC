{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "studentId",
        "isPresent",
        "schoolId",
        "grade",
        "section"
    ],
    "properties": {
        "studentId": {
            "type": "integer",
            "default": 0,
            "title": "The studentId Schema",
            "examples": [
                123
            ]
        },
        "isPresent": {
            "type": "boolean",
            "default": false,
            "title": "The isPresent Schema",
            "examples": [
                true
            ]
        },
        "schoolId": {
            "type": "integer",
            "default": 0,
            "title": "The schoolId Schema",
            "examples": [
                1234567890
            ]
        },
        "grade": {
            "type": "integer",
            "default": 0,
            "title": "The grade Schema",
            "examples": [
                1
            ]
        },
        "section": {
            "type": "string",
            "default": "",
            "title": "The section Schema",
            "examples": [
                "A"
            ]
        }
    },
    "examples": [
        {
            "studentId": 123,
            "isPresent": true,
            "schoolId": 1234567890,
            "grade": 1,
            "section": "A"
        }
    ]
}