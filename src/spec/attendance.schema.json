{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "isPresent",
        "schoolId",
        "grade",
        "sectionInGrade"
    ],
    "properties": {
        "isPresent": {
            "type": "boolean",
            "default": false,
            "title": "The isPresent Schema",
            "examples": [
                true
            ]
        },
        "schoolId": {
            "type": "integer",
            "default": 0,
            "title": "The schoolId Schema",
            "examples": [
                1234567890
            ]
        },
        "grade": {
            "type": "integer",
            "default": 0,
            "title": "The grade Schema",
            "examples": [
                1
            ]
        },
        "sectionInGrade": {
            "type": "string",
            "default": "",
            "title": "The sectionInGrade Schema",
            "examples": [
                "A"
            ]
        }
    },
    "examples": [
        {
            "isPresent": true,
            "schoolId": 1234567890,
            "grade": 1,
            "sectionInGrade": "A"
        }
    ]
}